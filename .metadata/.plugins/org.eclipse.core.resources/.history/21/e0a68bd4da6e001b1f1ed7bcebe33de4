package com.example.jpa.dao;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Query;
import org.springframework.data.repository.query.Param;
import org.springframework.stereotype.Repository;

import com.example.jpa.entities.Person;
import com.example.jpa.entities.User1;
@Repository
public interface  UserDao extends JpaRepository<Person, Integer>{
	public User1 findByUserName(String name);
@Query(value = "Select * from User1 where user_name=:n",nativeQuery=true)
	public User1 getData(@Param("n") String name);
}
